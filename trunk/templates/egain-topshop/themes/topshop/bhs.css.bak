        font-size: 11px;
    }

    &.active {

        border-color : @message-bg-color;
        background : #feedc1;
    }
}

/*** WRAPPERS ****/

/* The most outer DIV of the box wrapper */
.egce-box-wrap {
    
    border : 1px solid #ccc; 

    /* The DIV for the box header */
    .egce-box-header {

        padding : 10px; 
        background : #808080;
        position : relative;
        height: 30px;
        
        /* The heading text for the heading of the box. */
        .egce-box-heading {

            font-size : @font-size-heading;
            font-weight : bold;
            font-family: Arial;
            float : left; 
            margin-top: 3px;
        }

        /* The heading text for the heading of the box. */
        .egce-box-side {

            font-size : 11px;
            float : right; 
            color : #fff;
            margin-top : 3px;
        }

        .egce-box-icon {
            
            position : absolute;
            top : 8px;
            right : 10px;
            display : block;
            font-size : @font-size-icon;
            color : @font-color;
        }
            
        //flyout image
        img.flyout {

            position : absolute;
            right : 0;
            bottom : -12px;
        }
    }

    .egce-box-content {
        
             
    }

    /** Set the different types **/
    &.egce-primary {

        .egce-box-header {

            	padding : 10px; 

            .egce-box-heading {

                color : @inverted-text;
            }
        }

        .egce-box-content {

             
        }

    }

    &.egce-highlighted {

        .egce-box-header {
  
            .egce-box-heading {

                color : @inverted-text;
            }
        }
    }

    &.egce-secondary {
    
        .egce-box-header {
 
            color : #fff; 

            .egce-box-heading {

                color : @inverted-text;
            }
        }

        .egce-box-content {
            background : #fff;
        }
    }
}

a.egce-link {

    color : #333; 

    &:hover {

        color : @highlighted-color;
    }
}
.egce-anchor-link {

	a:link {
		color: @anchor-link-color; 
