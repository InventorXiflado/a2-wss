//Button
.egce-button {
     
    font-size : @font-size-medium;

    //display : inline;
    cursor : pointer;
    border-color : @highlighted-gradient-start;
    
    &.egce-primary {

        background : @primary-color;
        .gradient(@highlighted-gradient-start, @highlighted-gradient-stop);
        padding : 10px;
        color : @light-text;
    }

    &.egce-secondary {

        background : @secondary-color;
    }
}

.egce-errormsg {

	.egce-p15;
	.egce-font-medium;
	.egce-border-bottom;
	color:red;
}

.egce-dropdown {

	float:left;
	.egce-font-medium;
	cursor : pointer;
	background : @highlighted-gradient-stop;
	color : @light-text; 
	box-shadow: 1px 1px 4px 0px #ccc;
	
	&:hover { 
	}
	
	.egce-dropdown-option {
		.egce-p10-vertical;
		.egce-p10-left
	}
	
	.egce-selected-option {
		color: @highlighted-gradient-stop;
	}
	
	.current-dropdown-state {  
		font-weight:bold;
		width:105px;
		float:left;
		position: relative;
	}
	
	.current-dropdown-state > ul {
 		.egce-border;
 		width:100%;
		position: absolute;
		background : @highlighted-gradient-stop;
		color : @light-text;
		.rounded-bottom-corners;
		box-shadow: 1px 1px 6px 0px #ccc;
 		z-index:1001;	
	}
	
	.current-dropdown-state ul {
		background-color:@primary-color; 
		color:black;
		font-weight:bold;
		display: none;
	}
	
	.current-dropdown-state:hover > ul {
		display: block;
	}
	
	.current-dropdown-state ul li:hover {
		.egce-bg-light;
	}
	
	.egce-drop-down-symbol {
		.egce-p10-top;
		.egce-p10-right;
		.egce-float-right;
	}
	
	.icon-ok {
		display:none;
	}
	
	.egce-selected-option .icon-ok {
		display:block;
	}
	
}


