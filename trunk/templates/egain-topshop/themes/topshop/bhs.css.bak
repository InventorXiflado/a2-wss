//regular list
ul.egce-list {
    
    z-index : 10000;
    *display: inline-block;
    *zoom: 1;

    li.egce-list-row {
        
        position : relative;
        z-index : 1000;
	a{	
		&:hover {
 			color: #f23e39;
		}
        }
    }

    &.egce-horizontal {
        
        overflow : hidden;

        li.egce-list-row {
            
            float : left;
        }
    }

    &.egce-vertical {
        
        &.egce-multilevel {
            
            ul.egce-child-list {
                
                display : none;
                position : absolute;
                top : -1px;
                left : 100%;
                border : @light-border;
            }

            li.egce-child-row {
                
                width : 200px;
                position : relative;
                background : @secondary-light-background;
                color : #000;

                &:hover {

                    background : @highlighted-gradient-start;
                }
            }
        }
    }
    
    li.selfservice-article-preview-row {
    	z-index: 999;
        *zoom : 1;
    }
}

//Ruled list
ul.egce-ruled {
    
    
    li.egce-list-row {

        border-bottom : 1px solid #ccc; 
    }
}


/** INPUT **/

.egce-input-text {
     
    border : 1px solid #ccc;
    position: relative;
    outline : none;
     
    input {

        font-family : @font-family;
        border : 0;
        background : none;
